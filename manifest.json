{
    "manifest_version": 2,
    "name": "Like",
    "version": "1.0",
    "description": " EnKiuBi",
    "icons": {
        "16": "logo.png",
        "48": "logo.png",
        "128": "logo.png"
    },
    "author": "Nguyễn Quang Bảo",
    "browser_action": {
        "default_icon": {
            "16": "logo.png",
            "24": "logo.png"

        },
        "default_title": " EnkiuBi",
        "default_popup": "popup.html"
    },

    "content_scripts": [{
        "matches": [
            "https://www.facebook.com/*"
        ],
        "js": ["jquery.js", "content.js"]
    }]
}